h.ceil(f*responsiveMultipler)end;local g={}function addNotification(h,i)if not h then return end;msg2=nil;if i then g.color=i else g.color={255,0,0}end;local j=math.max(dxGetTextWidth(h,0.85,"default-bold"),dxGetTextWidth("",0.85,"default-bold"))+20;local k=j;g.tileWidth=k;g.tileHeight=math.max(dxGetFontHeight(0.85,"default-bold")*1+10,respc(40))g.tilePosX=(screenX-k)/2;g.moveDownTick=getTickCount()g.moveUpTick=g.moveDownTick+1000+(h and utfLen(h)or 0)*125;g.state=true;g.message={h}end;addEventHandler("onClientRender",getRootElement(),function()if not g or not g.state then return end;local l=getTickCount()local a=g.tilePosX;local m=-g.tileHeight;local n=0;if l>=g.moveDownTick and l<=g.moveUpTick then n,m=interpolateBetween(0,-g.tileHeight,0,1,50,0,(l-g.moveDownTick)/500,"OutQuad")elseif l>=g.moveUpTick then local o=(l-g.moveUpTick)/500;n,m=interpolateBetween(1,50,0,0,-g.tileHeight,0,o,"OutQuad")if o>1 then g.state=false end end;dxDrawRectangle(a,m,g.tileWidth,g.tileHeight,tocolor(50,50,50,200*n),true)dxDrawRectangle(a+2,m+2,g.tileWidth-4,g.tileHeight-4,tocolor(25,25,25,175*n),true)local o=(l-g.moveDownTick)/(g.moveUpTick-g.moveDownTick)if o<=1 then dxDrawRectangle(a,m+g.tileHeight-2,g.tileWidth*(1-o),2,tocolor(g.color[1],g.color[2],g.color[3],255*n),true)end;dxDrawText(g.message[1],a,m,a+g.tileWidth,m+g.tileHeight,tocolor(255,255,255,n*255),0.85,"default-bold","center","center",true,false,true)end,true,"low-999999999")addNotification("SHINE 1.3 Has Been Loaded Successfully.",{255,0,0})local p,q={},{"Linear","InQuad","OutQuad","InOutQuad","OutInQuad","InElastic","OutElastic","InOutElastic","OutInElastic","InBack","OutBack","InOutBack","OutInBack","InBounce","OutBounce","InOutBounce","OutInBounce","SineCurve","CosineCurve"}function table.find(r,s)for t,u in ipairs(r)do if u==s then return t end end;return false end;function animate(v,r,w,x,y,z)assert(type(v)=="number","Bad argument @ 'animate' [expected number at argument 1, got "..type(v).."]")assert(type(r)=="number","Bad argument @ 'animate' [expected number at argument 2, got "..type(r).."]")assert(type(w)=="string"or type(w)=="number"and(w>=1 or w<=#q),"Bad argument @ 'animate' [Invalid easing at argument 3]")assert(type(x)=="number","Bad argument @ 'animate' [expected number at argument 4, got "..type(x).."]")assert(type(y)=="function","Bad argument @ 'animate' [expected function at argument 5, got "..type(y).."]")table.insert(p,{from=v,to=r,easing=table.find(q,w)and w or q[w],duration=x,start=getTickCount(),onChange=y,onEnd=z})return#p end;addEventHandler("onClientRender",root,function()local A=getTickCount()for t,s in ipairs(p)do s.onChange(interpolateBetween(s.from,0,0,s.to,0,0,(A-s.start)/s.duration,s.easing))if A>=s.start+s.duration then if type(s.onEnd)=="function"then s.onEnd()end;table.remove(p,t)end end end)function isEventHandlerAdded(B,C,D)if type(B)=='string'and isElement(C)and type(D)=='function'then local E=getEventHandlers(B,C)if type(E)=='table'and#E>0 then for F,s in ipairs(E)do if s==D then return true end end end end;return false end;function toggleShine()guiSetVisible(ShineBG,not guiGetVisible(ShineBG))showCursor(guiGetVisible(ShineBG))if guiGetVisible(ShineBG)then animate(0,0.8,1,800,function(n)guiSetAlpha(ShineBG,n)end)end end;bindKey(tostring(guiGetText(Keybind)),"down",toggleShine)function fastSprint()setPedAnimationSpeed(localPlayer,"run_player",3.5)setPedAnimationSpeed(localPlayer,"SPRINT_civi",3.5)end;function fastReload()setPedAnimationSpeed(localPlayer,"buddy_crouchreload",10)setPedAnimationSpeed(localPlayer,"buddy_reload",10)setPedAnimationSpeed(localPlayer,"colt45_crouchreload",10)setPedAnimationSpeed(localPlayer,"colt45_reload",10)setPedAnimationSpeed(localPlayer,"sawnoff_reload",10)setPedAnimationSpeed(localPlayer,"python_crouchreload",10)setPedAnimationSpeed(localPlayer,"python_reload",10)setPedAnimationSpeed(localPlayer,"RIFLE_crouchload",10)setPedAnimationSpeed(localPlayer,"RIFLE_load",10)setPedAnimationSpeed(localPlayer,"CrouchReload",1