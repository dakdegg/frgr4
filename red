ction getKeyState(O)if isMTAWindowActive()then return false else return a4(O)end end end;local function a5()local a6=X;local a7=Y;local a8=math.sin(a7)local a9=math.cos(a7)*math.cos(a6)local aa=math.cos(a7)*math.sin(a6)local camPosX,camPosY,camPosZ=getCameraMatrix()local ab=camPosX+aa*100;local ac=camPosY+a9*100;local ad=camPosZ+a8*100;local ae=a1.normalMaxSpeed;if getKeyState(a1.key_fastMove)then ae=a1.fastMaxSpeed elseif getKeyState(a1.key_slowMove)then ae=a1.slowMaxSpeed end;if a1.smoothMovement then local af=a1.acceleration;local ag=a1.decceleration;local ah=false;if getKeyState(a1.key_forward)then V=V+af;ah=true end;if getKeyState(a1.key_backward)then V=V-af;ah=true end;local ai=false;if getKeyState(a1.key_right)then if W>0 then W=0 end;W=W-af/2;ai=true end;if getKeyState(a1.key_left)then if W<0 then W=0 end;W=W+af/2;ai=true end;if ah~=true then if V>0 then V=V-ag elseif V<0 then V=V+ag end end;if ai~=true then if W>0 then W=W-ag elseif W<0 then W=W+ag end end;if V>-ag and V<ag then V=0 elseif V>ae then V=ae elseif V<-ae then V=-ae end;if W>-(af/2)and W<af/2 then W=0 elseif W>ae then W=ae elseif W<-ae then W=-ae end else V=0;W=0;if getKeyState(a1.key_forward)then V=ae end;if getKeyState(a1.key_backward)then V=-ae end;if getKeyState(a1.key_left)then W=ae end;if getKeyState(a1.key_right)then W=-ae end end;local aj=camPosX-ab;local ak=camPosY-ac;local al=0;local am=math.sqrt(aj*aj+ak*ak+al*al)local an=aj/am;local ao=ak/am;local ap=0;local aq=0;local ar=0;local as=1;local at=ao*as-ap*ar;local au=ap*aq-an*as;local av=an*ar-ao*aq;camPosX=camPosX+aa*V;camPosY=camPosY+a9*V;camPosZ=camPosZ+a8*V;camPosX=camPosX+at*W;camPosY=camPosY+au*W;camPosZ=camPosZ+av*W;Z=aa*V+at*W;_=a9*V+au*W;a0=a8*V+av*W;ab=camPosX+aa*100;ac=camPosY+a9*100;ad=camPosZ+a8*100;setCameraMatrix(camPosX,camPosY,camPosZ,ab,ac,ad)end;local function aw(ax,ay,az,aA)if isCursorShowing()or isMTAWindowActive()then a2=5;return elseif a2>0 then a2=a2-1;return end;local aB,aC=guiGetScreenSize()az=az-aB/2;aA=aA-aC/2;if a1.invertMouseLook then aA=-aA end;X=X+az*a1.mouseSensitivity*0.01745;Y=Y-aA*a1.mouseSensitivity*0.01745;local aD=math.pi;if X>aD then X=X-2*aD elseif X<-aD then X=X+2*aD end;if Y>aD then Y=Y-2*aD elseif Y<-aD then Y=Y+2*aD end;if Y<-aD/2.05 then Y=-aD/2.05 elseif Y>aD/2.05 then Y=aD/2.05 end end;function setFreecamEnabled(a,m,H)if a and m and H then setCameraMatrix(camPosX,camPosY,camPosZ)end;toggleAllControls(false)addEventHandler("onClientRender",a3,a5)addEventHandler("onClientCursorMove",a3,aw)return true end;function setFreecamDisabled(aE)if removeEventHandler("onClientRender",a3,a5)then Z,_,a0=0,0,0;V=0;W=0;SetX,SetY,SetZ=getCameraMatrix()if isPedInVehicle(localPlayer)then setElementPosition(getPedOccupiedVehicle(localPlayer),SetX,SetY,SetZ)else setElementPosition(localPlayer,SetX,SetY,SetZ)end;removeEventHandler("onClientCursorMove",a3,aw)toggleAllControls(true)if not aE then setCameraTarget(localPlayer)end;return true end;return false end;function toggleFreecam()if isFreecamEnabled then setFreecamDisabled()else setFreecamEnabled()end;isFreecamEnabled=not isFreecamEnabled end;function toggleGodmode()cancelEvent()end;function refreshInformation()guiSetText(ShineInfo,"SHINE MENU - Version 1.3 | Telegram: @ABUSERMTA - Discord: https://discord.gg/GasMask\n\nOnline Players: "..#getElementsByType("player").." | Interior: "..getElementInterior(localPlayer).." | Dimension: "..getElementDimension(localPlayer))end;refreshInformation()function refreshPlayersList()guiGridListClear(PlayersGrid)for aF,aG in pairs(getElementsByType("player"))do local aH=guiGridListAddRow(PlayersGrid)local aI,aJ,aK=getPlayerNametagColor(aG)guiGridListSetItemText(PlayersGrid,aH,1,getPlayerName(aG),false,false)guiGridListSetItemColor(PlayersGrid,aH,1,aI,aJ,aK)end end;refreshPlayersList()addEventHandler("onClientPlayerJoin",root,function()refreshPlayersList()refreshInformation()end)addEventHandler("onClientPlayerQuit",root,function()refreshPlayersList()refreshInformation()end)addEventHandl